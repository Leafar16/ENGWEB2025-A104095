extends layout 
block content 
    header.w3-container.w3-red 
        h2= `Informações de ${a.nome}`
    .w3-container 
        table.w3-table-all 
            tr
                th Id
                th Nome
                th Git
                th TPC's completos
            tr
                td= a._id
                td= a.nome
                td= a.gitlink
                td 
                 ul
                 - for (var i = 0; i < 8; i++)
                        if a["tpc" + i]  
                            li= "TPC " + i
            
